{
    "schemaVersion": 1,
    "id": "meh",
    "version": "${version}",
    "name": "Miscellaneous Enhancements for Hypixel (MEH)",
    "description": "A client-sided mod to enhance the Hypixel Minecraft server experience by adding a few miscellaneous additions.\n\nThis mod contains whatever feature that feels like it would be useful while playing on Hypixel without making the game feel overly modded for any reason. These are mostly convenience additions!",
    "authors": [
        "Choco"
    ],
    "contact": {
        "sources": "https://github.com/2008Choco/MEH",
        "homepage": "https://github.com/2008Choco/MEH",
        "issues": "https://github.com/2008Choco/MEH/issues"
    },
    "license": "MIT",
    "icon": "assets/meh/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "wtf.choco.meh.client.MEHClient"
        ],
        "fabric-datagen": [
            "wtf.choco.meh.datagen.MEHDataGenerator"
        ],
        "modmenu": [
            "wtf.choco.meh.client.config.MEHModMenuIntegration"
        ]
    },
    "mixins": [
        {
            "config": "meh.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "fabricloader": ">=0.16",
        "minecraft": ">=1.21.1",
        "java": ">=21",
        "fabric-api": ">=0.104",
        "cloth-config": ">=15"
    },
    "suggests": {
        "modmenu": ">=11"
    },
    "custom": {
        "modmenu": {
            "update_checker": false
        }
    }
}
